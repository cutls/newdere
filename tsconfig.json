{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "useUnknownInCatchVariables": true,
  }
}